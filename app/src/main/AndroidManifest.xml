<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.cs.sdsu.jonbeacher.assignment3a" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >

        <activity
            android:name=".ProfessorListActivity"
            android:label="@string/professor_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ProfessorDetailActivity"
            android:label="@string/professor_details" >
        </activity>

        <activity
            android:name=".ProfessorCommentsActivity"
            android:label="@string/professor_comments" >
        </activity>

        <activity
            android:name=".ProfessorRateActivity"
            android:label="@string/professor_rate" >
        </activity>

    </application>

</manifest>
